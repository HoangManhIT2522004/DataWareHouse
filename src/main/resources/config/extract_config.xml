         CONTROL DATABASE CONFIGURATION
         ==================================================================== -->
    <control>
        <url>jdbc:postgresql://localhost:5432/control</url>
        <username>postgres</username>
        <password>your_password</password>
    </control>

    <!-- ====================================================================
         WEATHER API CONFIGURATION
         ==================================================================== -->
